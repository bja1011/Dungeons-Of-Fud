{
  "name": "angular-phaser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node prepare-assets.js && ng serve",
    "build": "node prepare-assets.js && ng build --prod",
    "deploy": "node prepare-assets.js && ng build --prod && firebase deploy",
    "test": "node prepare-assets.js && ng test",
    "lint": "node prepare-assets.js && ng lint",
    "e2e": "node prepare-assets.js && ng e2e",
    "doc": "compodoc -p ./tsconfig.json",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config .prettierrc --list-different \"src/{app,environments,assets,styles}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:all": "prettier --config .prettierrc --write \"src/{app,environments,assets,styles}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.4.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "compodoc": "0.0.41",
    "core-js": "^2.5.4",
    "dat.gui": "^0.7.2",
    "phaser": "^3.12.0-beta2",
    "phaser-animated-tiles": "^2.0.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "2.1.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
